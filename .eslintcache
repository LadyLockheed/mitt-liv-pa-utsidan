ia av allEquipment så att ändringarna inte sker i originallistan\r\n        setExpandedItems(equipmentList.map(equipment =>{\r\n\r\n            return{...equipment, isExpanded:false}\r\n        }))\r\n\r\n    },[])\r\n   \r\n    const toggleOpen = (item) => {\r\n        setDisplayModal(false);\r\n        setExpandedItems(expandedItems.map((equipment)=>{\r\n            \r\n            if(equipment !== item ){\r\n                return equipment\r\n            } else {\r\n                return {...equipment, isExpanded:!equipment.isExpanded}\r\n            }\r\n                  \r\n        }))\r\n    }\r\n\r\n    const deleteEquipment = () =>{\r\n        // setDisplayModal(false)\r\n        console.log('Equipment deleted')\r\n    }\r\n\r\n    const editEquipment = (equipment) =>{\r\n        console.log('equipment edited')\r\n        console.log(equipment)\r\n        \r\n    }\r\n    return(\r\n   \r\n        <Wrapper>\r\n        {expandedItems.map((item, index)=>{\r\n            return (\r\n                    <ItemWrapper key={item.equipment+index} >\r\n                        <TopRowWrapper onClick={()=>toggleOpen(item)}>\r\n                            <CategoryDot categoryColor={item.category}/>\r\n                            <Name>{item.equipment}</Name>\r\n                            <Weight>{item.weight}g</Weight>\r\n                            <DropDownArrow src={dropDownArrow}  isUpsideDown={item.isExpanded}></DropDownArrow>\r\n                        </TopRowWrapper>\r\n                        {item.isExpanded &&\r\n                        \r\n                            <BottomRowWrapper>\r\n                                <Info>{item.info}</Info>\r\n                                <IconWrapper>\r\n\r\n                                    <EditIcon src={editIcon} onClick={() => editEquipment(item._id)}/>\r\n                                    \r\n                                    <TrachcanIcon src={trashcanIcon} onClick={()=> setDisplayModal(!displayModal)}></TrachcanIcon>\r\n\r\n                                    {displayModal && \r\n                                    <AlertModal \r\n                                    setDisplayModal = {setDisplayModal} \r\n                                    displayModal = {displayModal}\r\n                                    headline = {'Are u sure?'}\r\n                                    confirmFunction = {deleteEquipment}/>\r\n                                    }\r\n                                    \r\n                                </IconWrapper>\r\n            \r\n                            </BottomRowWrapper>\r\n                        }\r\n            \r\n        \r\n                    </ItemWrapper>\r\n                )\r\n        })}\r\n\r\n       </Wrapper>\r\n       \r\n   \r\n      \r\n    )\r\n}\r\n\r\n\r\nexport default Accordion\r\n\r\n","C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Shared\\ButtonsAndSuch.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Menu\\Burger.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Menu\\MenuFromSide.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Login\\Login.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Login\\AddNewUser.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Shared\\GlobalStates.jsx",[],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbeten\\Examensarbete Mitt liv på utsidan\\mitt-liv-pa-utsidan\\src\\Components\\Gallery.jsx",[],["116","117"],"C:\\Users\\Lockheed\\Frontend\\Skolan\\Projektarbe